{
  "name": "TicTacToe_Typescript",
  "version": "1.0.0",
  "description": "A Tic Tac Toe board game in Typescript",
  "main": "index.js",
  "scripts": {
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chokonaira/TicTacToe_Typescript.git"
  },
  "keywords": [],
  "author": "Henry Okonkwo",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/chokonaira/TicTacToe_Typescript/issues"
  },
  "homepage": "https://github.com/chokonaira/TicTacToe_Typescript#readme",
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@babel/register": "^7.12.1",
    "@types/jasmine": "^3.6.1",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.9",
    "jasmine": "^3.6.3",
    "jasmine-spec-reporter": "^6.0.0",
    "jest": "^26.6.3",
    "ts-jest": "^26.4.4",
    "typescript": "^4.0.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testMatch": [
      "**/*.(test|spec).(ts|tsx)"
    ],
    "globals": {
      "ts-jest": {
        "babelConfig": true,
        "tsconfig": "jest.tsconfig.json"
      }
    },
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "text-summary"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/mocks.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/mocks.js"
    }
  }
}
